integer_Var=10
float_var=3.14
string="Hello"
list_var=[1,2,3]
tuple_var=(1,2,3)
dict_var={"Name":"John","Role":"Engineer"}
bool_var=True
#Print and manipulate Variables
print("Integer:",integer_Var)
print("Float:",float_var)
print("String:",string)
list_var.append(4)
print("List:",list_var)
print("Tuple:",tuple_var)
print("Dictionary:",dict_var)
print("Dictionary:",dict_var["Name"])
print("Boolean:",bool_var)
